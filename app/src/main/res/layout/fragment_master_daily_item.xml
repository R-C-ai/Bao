    xmlns:tools="http://schemas.android.com/tools">

    <data>
<variable
    name="viewModel"
    type="com.hsiaoling.bao.master.dailyItem.MasterDailyItemViewModel" />
    </data>

        <androidx.constraintlayout.widget.ConstraintLayout
